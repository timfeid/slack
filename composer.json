{
    "name": "timfeid/slack",
    "description": "Slack integration with PHP",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Tim Feid",
            "email": "tim@timfeid.com"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "guzzlehttp/guzzle": "~6.0",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.2.*",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*"
    },
    "autoload": {
        "psr-4": {
            "TimFeid\\Slack\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "minimum-stability": "stable"
}
